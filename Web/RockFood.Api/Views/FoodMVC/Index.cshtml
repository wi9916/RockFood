@model IEnumerable<RockFood.Interfaces.IFoodable>
@{
    Layout = "_Layout";
}
@using RockFood.Api.App_Code
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.About)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Price)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Count)
                </th>
                <th></th>
            </tr>
        </thead>
        @Html.CreateList(Model, "FoodMVC")
        
    </table>
    <p>
        <a asp-action="Create">Create New</a>
    </p>
</body>
</html>
